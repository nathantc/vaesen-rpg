{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "object-curly-spacing": "off",
        "react/react-in-jsx-scope": "off",
        "space-before-function-paren": "off",
        "semi": "off"
    },
    "settings": {
        "react": {
            "version": "999.999.999"
        }
    }
}
