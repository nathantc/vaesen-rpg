{
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/*?post_login_redirect_uri=/",
      "statusCode": 302
    }
  },
  "routes": [
    {
      "route": "/.auth/login/twitter",
      "statusCode": 404
    },
    {
      "route": "/api/*",
      "allowedRoles": [
        "authenticated"
      ]
    }
  ]
}